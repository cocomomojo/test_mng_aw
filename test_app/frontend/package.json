{
  "name": "test-app-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "setup:e2e": "playwright install --with-deps",
    "setup:agents": "playwright init-agents --loop=vscode",
    "test": "npm run test:e2e",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:headless": "playwright test --headless",
    "test:allure": "playwright test --reporter=line,allure-playwright",
    "test:e2e:report": "allure generate allure-results -o allure-report --clean && allure open allure-report",
    "manual:generate": "bash ../scripts/generate-manual.sh",
    "manual:generate:user": "bash ../scripts/generate-manual.sh --type user",
    "manual:generate:admin": "bash ../scripts/generate-manual.sh --type admin",
    "manual:generate:ai": "bash ../scripts/generate-manual.sh --ai",
    "manual:generate:user:ai": "bash ../scripts/generate-manual.sh --type user --ai",
    "manual:generate:admin:ai": "bash ../scripts/generate-manual.sh --type admin --ai"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@mdi/font": "^7.0.96",
    "@playwright/test": "^1.40.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/compiler-sfc": "^3.3.4",
    "allure-commandline": "^2.13.9",
    "allure-playwright": "^2.0.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^25.0.1",
    "sass": "^1.70.0",
    "vite": "^5.0.0",
    "vitest": "^2.1.5",
    "vuetify": "^3.3.0"
  }
}
